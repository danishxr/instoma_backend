    "version": "0.2.0",
    "configurations": [
        {
            "name": "mainv1",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/mainv1.py",
            "python": "/opt/anaconda3/envs/myplugin1/bin/python",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "python": "/opt/anaconda3/envs/myplugin1/bin/python",
            "console": "integratedTerminal"
        },
        {
            "name": "insta_agent",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/ai/agents/instagram_tools/insta_agent.py",
            "python": "/opt/anaconda3/envs/myplugin1/bin/python",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "console": "integratedTerminal"
        },
    ]
}